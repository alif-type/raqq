{
code = "@beh = [$[name MATCHES \"behDotless-ar.(init|medi).*\" && ! name MATCHES \".*\\.(round|l1|l2|hah).*\"]];
@hah = [hah-ar.init hah-ar.medi hah-ar.medi.alt];
