code = "lookup ligatures {
	lookupflag IgnoreMarks;

	sub lam-ar.init @alef.fina by lam_alef-ar; # XXX rand lam_alef
	sub lam-ar.medi @alef.fina by lam_alef-ar.fina; # XXX rand

	sub behDotless-ar.init reh-ar.fina by behDotless_reh-ar;
	sub behDotless-ar.init @waw.fina by behDotless_waw-ar;  # XXX rand
	sub behDotless-ar.init seen-ar.medi by behDotless_seen-ar.init;

	sub @ain.init @waw.fina by ain_waw-ar;
	sub fehDotless-ar.init alefMaksura-ar.fina by fehDotless_alefMaksura-ar;
	sub fehDotless-ar.init @alef.fina by fehDotless_alef-ar; # XXX rand

	sub lam-ar.medi alefMaksura-ar.fina by lam_alefMaksura-ar.fina;

	sub meem-ar.init noonghunna-ar.fina by meem_noonghunna-ar;

	sub lam-ar.init lam-ar.medi heh-ar.fina by lam_lam_heh-ar;
} ligatures;

lookup contxtuals {
	lookupflag IgnoreMarks;

	sub behDotless_seen-ar.init @waw.fina' by waw-ar.fina.alt;

	sub behDotless-ar.medi' meem-ar.fina by behDotless-ar.medi.meem;

	sub behDotless-ar.init' ain-ar.medi by behDotless-ar.init.ain;

	sub ain-ar.medi lam-ar.medi' by lam-ar.medi.ain;

	sub @ain.init behDotless-ar.medi' by behDotless-ar.medi.ain;
	sub kaf-ar.init behDotless-ar.medi' by behDotless-ar.medi.kaf;
	sub lam-ar.init behDotless-ar.medi' by behDotless-ar.medi.lam;

	sub meem-ar.medi' @waw.fina by meem-ar.medi.alt;
	sub meem-ar.init' [meem-ar.medi] by meem-ar.init.alt;

	sub [meem-ar.init meem-ar.medi] @alef.fina' by alef-ar.fina.meem; # XXX rand
} contxtuals;
";
name = rlig;
